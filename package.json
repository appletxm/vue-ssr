{
  "name": "vue-ssr",
  "version": "0.1.0",
  "description": "just test vuejs server side render with nodejs and webpack",
  "main": "./src/js/app.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development node ./scripts/server.js",
    "start": "cross-env NODE_ENV=production node ./scripts/server.js",
    "build": "rimraf dist && npm run build:client && npm run build:server",
    "build:client": "cross-env NODE_ENV=production webpack --config config/webpack.cfg.client.js --progress --hide-modules",
    "build:server": "cross-env NODE_ENV=production webpack --config config/webpack.cfg.server.js --progress --hide-modules"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/appletxm/vue-ssr",
    "issue": "https://github.com/appletxm/vue-ssr/issues"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "cross-env": "^5.2.0",
    "express": "^4.16.3",
    "lru-cache": "^4.1.3",
    "vue": "2.5.17",
    "vue-router": "3.0.1",
    "vue-server-renderer": "2.5.17",
    "vuex": "3.0.1",
    "vuex-router-sync": "5.0.0"
  },
  "devDependencies": {
    "accepts": "^1.3.5",
    "any-promise": "^1.3.0",
    "autoprefixer": "^9.1.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-env": "1.7.0",
    "chokidar": "^2.0.4",
    "co": "^4.6.0",
    "content-disposition": "^0.5.2",
    "content-type": "^1.0.4",
    "cookies": "^0.7.1",
    "css-loader": "^1.0.0",
    "debug": "^3.1.0",
    "deep-equal": "^1.0.1",
    "delegates": "^1.0.0",
    "html-loader": "^0.5.5",
    "less": "^3.8.1",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.4.1",
    "postcss-loader": "^2.1.6",
    "rimraf": "^2.6.2",
    "style-loader": "^0.22.0",
    "url-loader": "^1.0.1",
    "vue-loader": "^15.3.0",
    "vue-template-compiler": "^2.5.17",
    "webpack": "4.16.5",
    "webpack-cli": "^3.1.0",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-hot-middleware": "^2.22.3",
    "webpack-merge": "^4.1.4",
    "webpack-node-externals": "^1.7.2"
  },
  "keywords": [
    "node js",
    "webpack",
    "vuejs",
    "vuejs server side render",
    "koa"
  ],
  "author": "applexm"
}
